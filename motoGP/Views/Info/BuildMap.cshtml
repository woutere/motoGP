
@{
    ViewData["Title"] = "Races on Map";

    <img src="~/images/allerlei/map.png" usemap="#worldmap" />

    <map name="#worldmap">
        @foreach (var race in ViewData["ListRaces"] as IEnumerable<Race>)
        {
            <area shape="circle" coords="@race.X,@race.Y,30" href="./ShowRace" alt="@race.RaceID" />
        }
    </map>
    
}

